<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <style name="botao">
        <item name="android:width">10dp</item>
        <item name="android:height">10dp</item>
    </style>
    
    <style name="botao1" parent="botao">
        <item name="android:background">#0066ff</item>
    </style>
    
    <style name="botao2" parent="botao">
        <item name="android:background">#ff0000</item>
    </style>

    <style name="botao3" parent="botao">
        <item name="android:background">#00ff00</item>
    </style>
    
    <style name="botao4" parent="botao">
        <item name="android:background">#ffcc00</item>
    </style>
    
    <style name="botao5" parent="botao">
        <item name="android:background">#bebebe</item>
    </style>    
    
    <style name="botao6" parent="botao">
        <item name="android:background">#464625</item>
    </style>
</resources>
